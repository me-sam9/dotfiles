super + c
	st -e cmus
super + e
	dmenu-unicode print
super + g
	toggle-filter
super + n
	passmenu --type
super + q
	xset r rate 250 50; setxkbmap -option caps:super
super + r
	st -e nnn -deH -T v
super + s
	librewolf
super + shift + t
	st -e trt
super + u
	setxkbmap us
super + shift + u
	setxkbmap latam
super + v
	dmenu-bookmarks print
super + shift + v
	bookmarkthis
super + x
	nsxiv -at ~/img
super + z
	zathura

XF86MonBrightnessUp
	brightness increase
XF86MonBrightnessDown
	brightness decrease
XF86AudioNext
	brightness increase
XF86AudioPrev
	brightness decrease

XF86AudioRaiseVolume
	volume increase
XF86AudioLowerVolume
	volume decrease
XF86AudioMute
	mute

XF86Calculator
	toggle-networking
Scroll_Lock
	backlight

Print
	maim | tee ~/img/screenshots/$(date +%Y-%m-%d:%H-%M-%S.png) | xclip -selection clipboard -t image/png
super + Print
	maim -s | tee ~/img/screenshots/$(date +%Y-%m-%d:%H-%M-%S.png) | xclip -selection clipboard -t image/png

super + Up 
	xrandr -o inverted
super + Left 
	xrandr -o right
super + Down 
	xrandr -o normal
super + Right 
	xrandr -o left

Menu
	xdotool click 1
shift + Menu
	xdotool click 2
