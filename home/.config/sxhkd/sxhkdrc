super + s
	librewolf
super + c
	st -e cmus
super + q
	st -e donut
super + z
	zathura
super + e
	dmenu-unicode print
super + v
	dmenu-bookmarks print
super + r
	st -e nnn -deHr
super + u
	setxkbmap us
super + y
	setxkbmap latam
super + o
	bookmarkthis
XF86MonBrightnessUp
	brightness increase
XF86MonBrightnessDown
	brightness decrease
XF86AudioNext
	brightness increase
XF86AudioPrev
	brightness decrease
XF86AudioRaiseVolume
	volume increase
XF86AudioLowerVolume
	volume decrease
XF86AudioMute
	mute
XF86Calculator
	toggle-networking
Scroll_Lock
	backlight
Print
	maim ~/img/screenshots/$(date +%Y-%m-%d:%H-%M-%S.png) | xclip -selection clipboard -t image/png -i $f
super + Print
	maim -s ~/img/screenshots/$(date +%Y-%m-%d:%H-%M-%S.png) | xclip -selection clipboard -t image/png -i $f
super + Up 
	xrandr -o inverted
super + Left 
	xrandr -o right
super + Down 
	xrandr -o normal
super + Right 
	xrandr -o left
super + x
	nsxiv -rat ~/img
super + n
	passmenu --type
Menu
	xdotool click 1
shift + Menu
	xdotool click 2
