Print
	maim ~/img/screenshots/$(date +%Y-%m-%d:%H-%M-%S.png) | xclip -selection clipboard -t image/png -i $f
super + Print
	maim -s ~/img/screenshots/$(date +%Y-%m-%d:%H-%M-%S.png) | xclip -selection clipboard -t image/png -i $f
super + Up 
	xrandr -o inverted
super + Left 
	xrandr -o right
super + Down 
	xrandr -o normal
super + Right 
	xrandr -o left
